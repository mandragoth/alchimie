{
    "name": "magia",
    "targetName": "magia",
    "targetType": "library",
    "license": "Zlib",
    "authors": [
        "Jonath",
        "Enalye"
    ],
    "copyright": "Copyright Â© 2022, Jonath, Enalye",
    "description": "Moteur de jeu Magia",
    "dependencies": {
        "bindbc-opengl": "~>0.17.1",
        "bindbc-sdl": "~>0.21.4",
        "grimoire": {
            "path": "../grimoire"
        }
    },
    "versions": [
        "GL_41",
        "GL_ARB",
        "SDL_202",
        "SDL_Image_204",
        "SDL_TTF_2014",
        "SDL_Mixer_204"
    ],
    "targetPath": "./bin$ARCH",
    "subPackages": [
        "./test/"
    ],
    "buildTypes": {
        "debug": {
            "buildOptions": [
                "debugMode",
                "debugInfo"
            ]
        },
        "release": {
            "buildOptions": [
                "releaseMode",
                "inline",
                "noBoundsCheck",
                "optimize"
            ]
        },
        "unittest": {
            "buildOptions": [
                "debugMode",
                "debugInfo",
                "unittests"
            ]
        }
    }
}