{
	"authors": [
		"Jonath",
		"Enalye"
	],
	"buildTypes": {
		"debug": {
			"buildOptions": [
				"debugMode",
				"debugInfo"
			]
		},
		"release": {
			"buildOptions": [
				"releaseMode",
				"inline",
				"noBoundsCheck",
				"optimize"
			]
		},
		"unittest": {
			"buildOptions": [
				"debugMode",
				"debugInfo",
				"unittests"
			]
		}
	},
	"copyright": "Copyright Â© 2022, Jonath, Enalye",
	"dependencies": {
		"bindbc-sdl": "~>1.3.3",
		"bindbc-opengl": "~>1.0.0",
		"bindbc-soloud": "~>0.1.3",
		"grimoire": {
			"path": "../grimoire"
		}
	},
	"description": "Moteur de jeu Magia",
	"license": "Zlib",
	"name": "magia",
	"targetName": "magia",
	"targetPath": "./bin$ARCH",
	"targetType": "library",
	"versions": [
		"GL_41",
		"GL_ARB",
        "BindSDL_Static",
        "SDL_2_26",
        "SDL_Image_2_6",
        "SDL_Mixer_2_6",
        "SDL_TTF_2_20"
	],
    "subConfigurations": {
        "bindbc-sdl": "static"
    },
    "libs-windows": [
        "../libs/$PLATFORM/$ARCH/SDL2",
        "../libs/$PLATFORM/$ARCH/SDL2_image",
        "../libs/$PLATFORM/$ARCH/SDL2_mixer",
        "../libs/$PLATFORM/$ARCH/SDL2_ttf"
    ],
    "lflags-posix": [
        "-L-L../libs/$PLATFORM/$ARCH/",
        "-L-lz"
    ],
    "libs-posix": [
        "SDL2",
        "SDL2_image",
        "SDL2_mixer",
        "SDL2_ttf"
    ]
}