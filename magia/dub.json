{
	"authors": [
		"Jonath",
		"Enalye"
	],
	"buildTypes": {
		"debug": {
			"buildOptions": [
				"debugMode",
				"debugInfo"
			]
		},
		"release": {
			"buildOptions": [
				"releaseMode",
				"inline",
				"noBoundsCheck",
				"optimize"
			]
		},
		"unittest": {
			"buildOptions": [
				"debugMode",
				"debugInfo",
				"unittests"
			]
		}
	},
	"copyFiles-windows": [
		"../libs/$PLATFORM/$ARCH/SDL2.dll",
		"../libs/$PLATFORM/$ARCH/SDL2_image.dll",
		"../libs/$PLATFORM/$ARCH/SDL2_ttf.dll",
		"../libs/$PLATFORM/$ARCH/OpenAL32.dll"
	],
	"copyright": "Copyright Â© 2022, Mandragoth",
	"dependencies": {
		"audio-formats": "~>2.2.0",
		"bindbc-openal": "~>1.0.0",
		"bindbc-opengl": "~>1.0.0",
		"bindbc-sdl": "~>1.3.3",
		"grimoire": {
			"path": "../grimoire"
		}
	},
	"description": "Moteur de jeu Magia",
	"lflags-posix": [
		"-L-L../libs/$PLATFORM/$ARCH/",
		"-L-lz"
	],
	"libs-posix": [
		"SDL2",
		"SDL2_image",
		"SDL2_mixer",
		"SDL2_ttf"
	],
	"libs-windows": [
		"../libs/$PLATFORM/$ARCH/SDL2",
		"../libs/$PLATFORM/$ARCH/SDL2_image",
		"../libs/$PLATFORM/$ARCH/SDL2_mixer",
		"../libs/$PLATFORM/$ARCH/SDL2_ttf",
		"../libs/$PLATFORM/$ARCH/OpenAL32"
	],
	"license": "Zlib",
	"name": "magia",
	"subConfigurations": {
		"bindbc-sdl": "static"
	},
	"targetName": "magia",
	"targetPath": "./bin$ARCH",
	"targetType": "library",
	"versions": [
		"GL_41",
		"GL_ARB",
		"BindSDL_Static",
		"SDL_2_26",
		"SDL_Image_2_6",
		"SDL_Mixer_2_6",
		"SDL_TTF_2_20"
	]
}